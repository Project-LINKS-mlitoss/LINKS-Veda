{
	"name": "links-veda",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "bun db:generate && remix vite:build",
		"dev": "bun db:migrate-dev && npm-run-all --parallel db:studio vite",
		"vite": "vite --host",
		"lint": "biome check .",
		"format": "biome check --write . && prisma format",
		"start": "NODE_ENV=production node ./build/server/index.js",
		"typecheck": "tsc",
		"storybook": "storybook dev -p 6006 --ci",
		"build-storybook": "storybook build",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"db:generate": "prisma generate",
		"db:migrate": "prisma migrate deploy",
		"db:migrate-dev": "prisma migrate dev",
		"db:studio": "prisma studio",
		"api:generate": "openapi --input reearth-cms-api.yml --output app/api/cms --useUnionTypes --exportCore false --useOptions"
	},
	"prisma": {
		"seed": "node --loader ts-node/esm ./prisma/seed.mts"
	},
